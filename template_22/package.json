{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev-server": "dotenv -e .env.production -- next dev",
    "dev": "dotenv -e .env.development -- next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
